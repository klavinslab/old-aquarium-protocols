place safety
  protocol: "labw14/week1/safety.pl"
  group: "tamimeur"
  marked: true
  start: now()
  window: minutes(30)
end

place safety_find
  protocol: "labw14/week1/test.pdl"
  group: "tamimeur"
  start: now()
  window: minutes(1)
end

# forward transitions
transition [safety] => [test]    when !error(0) && completed(0) end
transition [test]     => [] when !error(0) && completed(0) && return_value(0,"numberWrong") == 1 end



# rework transitions
transition [safety] => [safety] when error(0) end
transition [test]     => [test]     when error(0) && completed(0) && return_value(0,"numberWrong") != 1 end

