<?xml version="1.0" encoding="UTF-8"?>
<protocol>
  <!-- Take a bottle -->
  <take>
    <item>
      <type>1 L Bottle</type>
      <quantity>1</quantity>
      <var>bottle</var>
    </item>
  </take>
  <!-- Display it to the turker / debugger -->
  <step>
    <description>A 1L Bottle item variable contains this info:</description>
    <note>%{bottle}</note>
  </step>
  <release>%{bottle}</release>
</protocol>
