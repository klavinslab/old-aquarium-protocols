<protocol>

  <information>Get plasmid DNA out of a cell sample.</information>

  <argument>
    <name>strain_sample_id</name>
    <type>number</type>
    <description>The id of the sample to use</description>
  </argument>

  <take>
    <item>
      <type>Liquid Culture</type>
      <quantity>1</quantity>
      <var>culture</var>
      <id>%{strain_sample_id}</id>
    </item>
  </take>

</protocol>