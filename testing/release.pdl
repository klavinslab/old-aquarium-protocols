<protocol>

  <take>
    <object>1000 mL Bottle</object>
    <quantity>2</quantity>
    <var>A</var>
  </take>

  <assign><lhs>method</lhs><rhs>%{A}[0][:object][:release_method]</rhs></assign>

  <information>First object release method: %{method}.</information>

  <release>A</release>
  
  <assign><lhs>released</lhs><rhs>%{A}[0][:object][:released]</rhs></assign>

  <information>First object release flag is %{released}.</information>

</protocol>
