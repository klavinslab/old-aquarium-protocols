<protocol>

  <take>
    <object>1000 mL Bottle</object>
    <quantity>2</quantity>
    <var>A</var>
  </take>

  <take>
    <object>Petri Dish</object>
    <quantity>1</quantity>
    <var>B</var>
  </take>

  <assign><lhs>num</lhs><rhs>%{A}.length</rhs></assign>
  <assign><lhs>method</lhs><rhs>%{A}[0][:object][:release_method]</rhs></assign>

  <information>%{num} Bottles taken. First object release method: %{method}.</information>

  <release>[%{A}[0],%{A}[1],%{B}[0]]</release>
  
  <information>Objects released.</information>

</protocol>


