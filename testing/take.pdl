<protocol>

    <argument>
      <name>object</name>
      <type>string</type>
      <description>An object to take from the IMS</description>
    </argument>

    <take>
        <object>%{object}</object>
        <quantity>2</quantity>
        <var>A</var>
    </take>

    <take>
        <object>1000 mL Bottle</object>
        <quantity>2</quantity>
        <var>B</var>
    </take>
    
    <information>Object information array: A = %{A} and B=%{B}.</information>
  
</protocol>
