<protocol>

    <argument>
      <name>object</name>
      <type>string</type>
      <description>An object to take from the IMS</description>
    </argument>

    <take>

      <item>
        <type>%{object}</type>
        <quantity>2</quantity>
        <var>A</var>
      </item>

      <item>
        <type>1000 mL Bottle</type>
        <quantity>2</quantity>
        <var>B</var>
      </item>

    </take>
    
    <information>Object information array: A = %{A} and B=%{B}.</information>
  
</protocol>
