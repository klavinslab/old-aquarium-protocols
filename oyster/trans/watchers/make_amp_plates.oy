place wait
    marked: true
end

place lb
    protocol: "oyster/trans/watcher/media/lb.pl"
    start: now()
    window: minutes(30)
    group: technicians
end

transition [wait] => [lb] when quantity("LB Amp Plate (sterile)") < min_quantity("LB Amp Plate (sterile)") end
transition [lb] => [wait] when completed(0) end
