<protocol name="Make Chloramphenicol Stock">
  <information>
  	This prepares 10ml of Chloramphenicol.
	</information>

	
    <take>
        <object>Electronic Serological Pipette</object>
	<quantity> 1 </quantity>
	<var> sero </var>
    </take>
    
    <take>
      <object> 10 ml serological pipette tip </object>
  <quantity> 1 </quantity>
	<var> sero </var>
    </take>

    <take>
        <object> Electronic Scale </object>
	<quantity> 1 </quantity>
	<var> pip </var>
    </take>
    
    <take>
	<object> 100 mL sterile Glass Bottle </object>
	<quantity> 1 </quantity>
	<var> bottle </var>
    </take>
    
    <take>
	<object> Chloramphenicol Salts </object>
	<quantity> 1 </quantity>
	<var> salts </var>
    </take>
    

    <take>
	<object> 100% ethanol </object>
	<quantity> 1 </quantity>
	<var> water </var>
    </take>
    
	    <take>
		<object> Eppendorf tubes </object>
		<quantity> 10 </quantity>
		<var> Eppen </var>
	    </take>

	

    <step>
	<description>
		Weigh out 0.34 grams of Chloramphenicol salts and add to 100 ml sterile bottle
	</description>
    </step>
	
    <step>
  <description>
		Use serological pipette to pipette 10 ml of 100% ethanol into 100 ml bottle
	</description>
    </step>
    
      <step>
  <description>
		Shake to mix well. If powder sticks, vortex.
	</description>
    </step>  
    
        <step>
  <description>
		Aliquot 1ml into each eppendorf tube
	</description>
    </step>
    
            <step>
  <description>
		Label "Chlor"
	</description>
    </step>
    
    
        <step>
  <description>
		Store eppendorf tubes in -20 freezer
	</description>
    </step>
    
    <release>
	<var> sero </var>
	<var> pip </var>
	<var> salts </var>
    </release>
    
 <produce>
        <object> Chloramphenicol aliquots. </object>
        <quantity>10</quantity>
    </produce>
    

    
</protocol>
